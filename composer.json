{
    "name": "akibatech/laravel-crudable",
    "description": "Laravel CRUD builder with ease",
    "keywords": ["laravel", "crud", "fields"],
    "license": "MIT",
    "type": "plugin",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "laracasts/testdummy": "~2.0",
        "laravel/laravel": ">=5.2"
    },
    "autoload": {
        "psr-4": {
            "Akibatech\\Crud\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/AbstractTestCase.php"
        ],
        "psr-4": {
          "TestModel\\": "tests/Models/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
